<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Notification resources -->
    <system:String x:Key="ChannelName">WindowsPhoneChannel</system:String>
    <system:String x:Key="ServiceName">PhoneApp</system:String>
    <system:String x:Key="ApplicationId">PhoneApp</system:String>
    <system:String x:Key="EndpointsServiceUri">http://localhost:23121/endpoints</system:String>
    <system:String x:Key="DefaultAllowedDomain">http://127.0.0.1</system:String>
    <system:String x:Key="DeviceType">WindowsPhone</system:String>

</ResourceDictionary>